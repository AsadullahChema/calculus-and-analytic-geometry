\addplot[name path=b,domain=-90:90]({2*f(x)},{2*g(x)})node[pos=0.75,above right]{$\begin{aligned}-\tfrac{\pi}{2}&\le\theta\le\tfrac{\pi}{2}\\  1&\le r\le 2  \end{aligned}$};
