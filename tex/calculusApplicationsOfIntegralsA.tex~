\draw[-latex](0,-0.2)--(0,2.6)node[above]{$y$};
\draw[gray,thick,-stealth]([shift={(-20:0.4cm and 0.2cm)}]0,2.45) arc (-20:240:0.4cm and 0.2cm);
\draw[]plot[domain=0:0.5]({\x},{f(\x)});
