\draw[-latex](0,-2)--(0,2)node[above]{$y$};
\draw(1,0)node[below left]{$1$}  (3,0)node[below left]{$3$}  (5,0)node[below right]{$5$};
\draw(0,{sqrt(2)})node[left]{$\sqrt{2}$}--++(0.1,0)  (0,{-sqrt(2)})node[left]{$-\sqrt{2}$}--++(0.1,0)  ;
