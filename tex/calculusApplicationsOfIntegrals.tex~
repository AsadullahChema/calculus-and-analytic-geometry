\draw(2,0)node[below]{$2$}--++(0,0.2);
\draw(0,{f(4/5)})node[circ]{}node[left]{$y$}--(4/5,{f(4/5)})node[circ]{}coordinate[pos=0.5](kkk);
\draw(kkk) to [out=-45,in=180]++(1,-0.5)node[right]{$R(y)=\frac{2}{y}$};
\end{tikzpicture}
