\draw(0,\c) circle(\fac cm and 0.4cm);
\draw(0,\c) circle(\fbc cm and 0.2cm);
\draw({-fa(\c)},\c)--++(0,-0.3);
\draw({fa(\c)},\c)--++(0,-0.3);
